<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Actions</key>
        <array>
            <dict>
                <key>Script Interpreter</key>
                <string>/usr/bin/env python</string>
                <key>Shell Script File</key>
                <string>comma-for-number.py</string>
                <key>Title</key>
                <string>Comma For Number</string>
                <key>After</key>
                <string>copy-result</string>
                <key>Regular Expression</key>
                <string>^\d{1,3}(?:(?:(,|)\d{3})(\1\d{3})*)?$</string>
            </dict>
        </array>
        <key>Extension Description</key>
        <string>Add or remove comma in numbers</string>
        <key>Extension Identifier</key>
        <string>com.study.jjmean2.popclip.extension.comma-for-number</string>
        <key>Extension Name</key>
        <string>Comma For Number</string>
        <key>Version</key>
        <integer>1</integer>
    </dict>
</plist>


